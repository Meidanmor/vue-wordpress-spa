V;yBAAvC,mBAAA,CAeM,KAfN,EAeM;AAfD,MAAA,KAAK,EAAC,QAeL;AAf8C,MAAA,GAAG,EAAE;AAenD,KAfN,E,CACE,mBAAA,CAWM,KAXN,EAAA,UAAA,EAWM,CAVJ,mBAAA,CAKM,KALN,EAAA,UAAA,EAKM,CAHI,IAAI,CAAC,SAAL,CAAc,kBAAd,C,kBADR,mBAAA,CAGE,KAHF,EAGE;YAAA;AADC,MAAA,GAAG,EAAE,IAAI,CAAC,SAAL,CAAc,kBAAd,EAAc,CAAd,EAAsC;AAC5C,KAHF,E,IAAA,E;;AAAA,M,UAAA,C,qCAII,CALN,CAUI,EAJJ,mBAAA,CAA+D,KAA/D,EAA+D;AAA1D,MAAA,KAAK,EAAC,cAAoD;AAArC,MAAA,SAA8B,EAAtB,IAAI,CAAC,OAAL,CAAa;AAAgB,KAA/D,E,IAAA,E;;AAAA,M,UAAA,CAII,EAHJ,mBAAA,CAEM,KAFN,EAAA,UAAA,EAEM,CADJ,mBAAA,CAAkC,IAAlC,EAAkC,IAAlC,EAAkC,gBAAA,CAA3B,IAAI,CAAC,KAAL,CAAW,QAAgB,CAAlC,EAA0B;AAAA;AAA1B,KACI,CAFN,CAGI,CAXN,C,EAYQ,YAAA,CACQ,sBADR,EACQ;AADM,MAAA,EAAE,EAAE,IAAI,CAAC,IACf;AADqB,MAAA,GAAG,EAAC,KACzB;AAD+B,MAAA,GAAG,EAAC;AACnC,KADR,E;wBAAqD;AAAA,eAC3D,C,UAAA,CAD2D;AAAA,O;;;;KAArD,E;;AAAA,M,MAAA,C,CAbV,C;GAeM,CAfN,E;;AAAA,GAgBI,EAjBN,CAkBM,CAnBV,C,mBAqBE,mBAAA,CAAwC,KAAxC,EAAA,UAAA,EAAwB,YAAxB,C,CAKM,EAJI,YAAA,CAGS,qBAHT,EAGS;AAHI,IAAA,UAAU,EAAE,KAAA,CAAA,UAGhB;AAFA,IAAA,MAAI,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA,EAAA;AAAA,QAAI,IAAA,CAAA,SAAA,CAAU,IAAd;AAAoB,MAAA,QAAA,CAAA,iBAAA;AAAiB,KAArC,CAEJ;AADA,IAAA,MAAI,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA,EAAA;AAAE,MAAA,IAAA,CAAA,SAAA,CAAU,IAAV;AAAkB,MAAA,QAAA,CAAA,iBAAA;AAAiB,KAArC;AACJ,GAHT,E,IAAA,E;;AAAA,I,cAAA,CAIJ,CA5BR,C","sourcesContent":["<template>\n<div>\n<template v-if=\"posts\">\n<div class=\"posts\">\n    <div class=\"row\">\n      <div class=\"column\" v-for=\"(post, index) in posts\" :key=\"index\">\n        <div class=\"card\">\n          <div class=\"card-image\">\n            <img\n              v-if=\"post._embedded['wp:featuredmedia']\"\n              :src=\"post._embedded['wp:featuredmedia'][0].source_url\"\n            />\n          </div>\n          <div class=\"card-content\" v-html=\"post.excerpt.rendered\"></div>\n          <div class=\"card-action\">\n            <h3>{{ post.title.rendered }}</h3>\n          </div>\n        </div>\n                <router-link :to=\"post.slug\" tag=\"div\" key=\"post.id\">\nREAD MORE </router-link>\n      </div>\n    </div>\n    </div>\n  </template>\n  <div id=\"loaded\" v-else>Loading...</div>\n            <pagination :pagination=\"pagination\"\n                    @prev=\"--postsData.page; getRecentMessages();\"\n                    @next=\"postsData.page++; getRecentMessages();\">\n        </pagination>\n  </div>\n</template>\n\n<script>\nimport pagination from \"../components/Pagination.vue\";\nimport axios from \"axios\";\nexport default {\n        components: {\n            'pagination': pagination\n        },\n  data() {\n    return {\n      // Wordpress Posts Endpoint\n      postsUrl: \"https://therapy.estetikinter.com/wp-json/wp/v2/posts\",\n      queryOptions: {\n        per_page: 5, // Only retrieve the 10 most recent blog posts.\n        page: 1, // Current page of the collection.\n        _embed: true //Response should include embedded resources.\n      },\n      pagination: {\n                    prevPage: '',\n                    nextPage: '',\n                    totalPages :  ' ' ,\n                    from: '',\n                    to: '',\n                    total: '',\n                },\n      // Returned Posts in an Array\n      posts: []\n    };\n  },\n  methods: {\n    // Get Recent Posts From WordPress Site\n    getRecentMessages() {\n      axios\n        .get(this.postsUrl, { params: this.queryOptions })\n        .then(response => {\n          this.posts = response.data;\n          this.configPagination(response.headers);\n          console.log(\"Posts retrieved!\");\n          console.log(this.posts);\n        })\n        .catch(error => {\n          console.log(error);\n        });\n    },\n            configPagination(headers) {\n                this.pagination.total = +headers['x-wp-total'];\n                this.pagination.totalPages = +headers['x-wp-totalpages'];\n                this.pagination.from = this.pagination.total ? ((this.postsData.page - 1) * this.postsData.per_page) + 1 : ' ';\n                this.pagination.to = (this.postsData.page * this.postsData.per_page) > this.pagination.total ? this.pagination.total : this.postsData.page * this.postsData.per_page;\n                this.pagination.prevPage = this.postsData.page > 1 ? this.postsData.page : '';\n                this.pagination.nextPage = this.postsData.page < this.pagination.totalPages ? this.postsData.page + 1 : '';\n            }\n  },\n  mounted() {\n    this.getRecentMessages();\n  }\n};\n</script>\n\n<style>\n#loaded{\n  font-size: 80px;\n  display: block;\n}\n\n.row {\n    display: grid !important;\n    grid-template-columns: 1fr 1fr;\n    grid-column-gap: 20px;\n    width: 100%;\n}\n\ndiv.card{\n  border: 2px solid black;\n  padding: 20px;\n}\n\n.card-image img {\n    width: 100%;\n    height: 200px;\n    object-fit: contain;\n}\n\n\n</style>"],"sourceRoot":""}]}